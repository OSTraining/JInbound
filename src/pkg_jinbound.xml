<?xml version="1.0" encoding="UTF-8" ?>
<extension type="package" version="2.5" method="upgrade">
	<name>PKG_JINBOUND</name>
	<license>http://www.gnu.org/licenses/gpl-2.0.html GNU/GPL</license>
	<packagename>jinbound</packagename>
	<version>@ant_version_number@</version>
	<author>Anything Digital</author>
	<authorEmail>admin@anything-digital.com</authorEmail>
	<authorUrl>http://anything-digital.com</authorUrl>
	<creationDate>@ant_current_date@</creationDate>
	<description>PKG_JINBOUND_DESCRIPTION</description>
	<scriptfile>jinbound.install.php</scriptfile>
	<updateservers>
		<server type="collection" priority="1" name="JInbound 2.x">https://jinbound.com/index.php?option=com_ars&amp;view=update&amp;task=category&amp;id=packages&amp;format=xml&amp;dummy=extension.xml</server>
	</updateservers>
	<files folder="packages">
		<file type="component" id="@ant_component_id@">@ant_component_package@</file>
		<file type="template" id="@ant_template_id@" client="site">@ant_template_package@</file>
		<file type="module" id="@ant_module_bookmark_id@" client="site">@ant_module_bookmark_package@</file>
		<file type="plugin" id="@ant_plugin_content_id@" group="content">@ant_plugin_content_package@</file>
		<file type="plugin" id="@ant_plugin_system_id@" group="system">@ant_plugin_system_package@</file>
		<file type="plugin" id="@ant_plugin_user_id@" group="user">@ant_plugin_user_package@</file>
	</files>
	<languages folder="language">
		<language tag="en-GB">en-GB/en-GB.pkg_jinbound.sys.ini</language>
	</languages>
</extension>